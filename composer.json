{
    "name": "drmmr763/laravel-asyncapi",
    "description": "Laravel package for AsyncAPI 3.0.0 specification with annotations and code generation",
    "keywords": [
        "drmmr763",
        "laravel",
        "laravel-asyncapi"
    ],
    "homepage": "https://github.com/drmmr763/laravel-asyncapi",
    "license": "MIT",
    "authors": [
        {
            "name": "Chad Windnagle",
            "email": "drmmr763@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0",
        "drmmr763/php-asyncapi-annotations": "dev-main",
        "symfony/yaml": "^6.0|^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^10.5.0||^9.5.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Drmmr763\\AsyncApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drmmr763\\AsyncApi\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse --memory-limit=256M",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Drmmr763\\AsyncApi\\AsyncApiServiceProvider"
            ],
            "aliases": {
                "AsyncApi": "Drmmr763\\AsyncApi\\Facades\\AsyncApi"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/drmmr763/php-asyncapi-annotations"
        }
    ]
}
